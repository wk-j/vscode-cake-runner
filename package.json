{
    "name": "cake-runner",
    "displayName": "Cake Runner",
    "description": "Execute Cake task from Visual Studio Code",
    "version": "0.0.1",
    "publisher": "wk-j",
    "engines": {
        "vscode": "^1.5.0"
    },
    "icon": "images/icon.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
