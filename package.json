{
  "name": "cake-runner",
  "displayName": "Cake Runner",
  "description": "Execute Cake task in Visual Studio Code",
  "version": "0.0.7",
  "publisher": "wk-j",
  "keywords": [
    "Cake",
    "Runner",
    "Build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wk-j/vscode-cake-runner"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "cakeRunner.showTasks",
        "title": "Cake Runner :: Show Tasks"
      }
    ],
    "keybindings": [
      {
        "command": "cakeRunner.showTasks",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t"
      }
    ]
  },
  "scripts": {
    "publish": "vsce publish",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {}
}
